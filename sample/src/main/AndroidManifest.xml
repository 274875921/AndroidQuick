<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="la.xiong.androidquick.demo">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".module.mvp.activity.MvpActivity" />
        <activity android:name=".module.db.DatabaseActivity" />
        <activity android:name=".module.image.ImageActivity" />
        <activity android:name=".module.bus.BusActivity" />
        <activity android:name=".module.ioc.IocActivity" />
        <activity
            android:name=".module.network.retrofit.DownloadActivity"
            android:theme="@style/ProgressBarTheme" />
        <service android:name=".module.network.retrofit.service.DownloadService" />
        <activity android:name=".function.ui.fragment.FragmentationActivity" />
        <activity android:name=".function.ui.bartop.ToolbarActivity" />
        <activity android:name=".function.ui.webview.WebViewActivity" />
        <activity android:name=".function.permission.PermissionActivity" />
        <activity android:name=".ui.activity.FrameActivity" />
        <activity android:name=".ui.activity.HostActivity" />
        <activity android:name=".ui.activity.ArchitectureActivity" />
        <activity android:name=".ui.activity.architecture3.WebViewJavascriptActivity" />

    </application>

</manifest>